{
  "version": 2,
  "builds": [
    {
      "src": "mobile/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "mobile/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/email-confirmed",
      "dest": "/mobile/public/email-confirmed.html"
    },
    {
      "src": "/(.*)",
      "dest": "mobile/dist/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_ENV": "production"
  }
}
