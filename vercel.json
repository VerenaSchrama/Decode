{
  "version": 2,
  "builds": [
    {
      "src": "mobile/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "mobile/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/email-confirmed",
      "dest": "/api/email-confirmed"
    },
    {
      "src": "/(.*)",
      "dest": "mobile/dist/$1"
    }
  ]
}