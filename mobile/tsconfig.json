{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "target": "ES2018",
    "lib": ["ES2018", "DOM", "ES6"],
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  }
}
